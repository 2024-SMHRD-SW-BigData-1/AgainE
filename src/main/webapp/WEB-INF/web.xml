  	<display-name>ItemTwoOptionSelectService</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.ItemTwoOptionSelectService</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JoinService</servlet-name>
  	<url-pattern>/join</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CheckUserIdService</servlet-name>
  	<url-pattern>/checkUserid</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
