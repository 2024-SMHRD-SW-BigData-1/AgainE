  	<url-pattern>/categorySelectItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ItemAllSelectService</servlet-name>
  	<url-pattern>/itemAll</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ItemColorSelectService</servlet-name>
  	<url-pattern>/colorSelectItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
