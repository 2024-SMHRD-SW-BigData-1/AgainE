	<script>
		document.getElementById('clickMe').addEventListener('click', function (event) {
			var balloon = document.getElementById('balloon');
			var clickMe = document.getElementById('clickMe');

			if (balloon.style.display === 'none' || balloon.style.display === '') {
				// 말풍선을 클릭한 요소 위로 이동
				balloon.style.display = 'block';
				var rect = clickMe.getBoundingClientRect();
				balloon.style.left = rect.left + window.scrollX + (rect.width / 2) - (balloon.offsetWidth / 2) + 'px';
				balloon.style.top = rect.top + window.scrollY - balloon.offsetHeight - 10 + 'px'; // 10px은 여유 공간
			} else {
				balloon.style.display = 'none';
			}
		});
	</script>
